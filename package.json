{
  "name": "password_generator",
  "productName": "Password Generator",
  "version": "1.0.0",
  "description": "Password Generator Desktop App",
  "main": "index.js",
  "scripts": {
    "start": "electron .",
    "package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --icon=assets/icon.icns --prune=true --out=release-builds",
    "package-win": "electron-packager . --overwrite --platform=win32 --arch=ia32 --icon=assets/icon.ico --prune=true --out=release-builds",
    "package-linux": "electron-packager . --overwrite --platform=linux --arch=x64 --icon=assets/icon.png --prune=true --out=release-builds"
  },
  "author": "Alan Gooding",
  "license": "ISC",
  "devDependencies": {
    "electron": "^19.0.8",
    "electron-packager": "^15.5.1"
  },
  "dependencies": {
    "rcedit": "^3.0.1"
  }
}
